{
  "manifest_version": 2,
  "name": "Web Tounge",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main/index.html",
    "default_title": "Web Tounge"
  }
}
